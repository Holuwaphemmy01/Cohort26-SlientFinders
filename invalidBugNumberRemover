function removeInvalidNumber(arrayOfBugsReport) {
  let validReport = [];
  for (let countValidEntry = 0; countValidEntry < arrayOfBugsReport.length; countValidEntry++) {
    let validBugs = [];

    for (let bugNumber of arrayOfBugsReport[countValidEntry]) {
      if (bugNumber <= 5) {
        validBugs.push(bugNumber);
      }
    }

    validReport.push(validBugs);
  }

  return validReport;
}

let bugsArray = [
  { name: "John", bugs: 7 },
  { name: "Abdul", bugs: 4 },
  { name: "Ahmed", bugs: 3 },
  { name: "Buhari", bugs: 5 },
  { name: "Bisi", bugs: 3 },
  { name: "Bola", bugs: 4 },
  { name: "Ify", bugs: 8 },
  { name: "Chioma", bugs: 0 }
];

console.log(removeInvalidNumber(bugsArray));
